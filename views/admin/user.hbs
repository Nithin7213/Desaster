<div class="container classData">
    <table class="table">
  <thead>
    <tr>
      <th scope="col">user ID</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">mobile</th>
      <th scope="col">Status</th>
      <th scope="col">Action</th>

      
    </tr>
  </thead>
  <tbody>
      {{#each user}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <th scope="row">{{this.userName}}</th>
      <td>{{this.email}}</td>
      <td>{{this.mobile}}</td>
      <td>{{this.type}}</td>
   
        <td><a href="/Userdelete/{{this.id}}" class="btn btn-outline-danger">Delete</a></td>
       {{!-- <td> <a href="/block/{{this.id}}" class="btn btn-danger">Block</a></td> --}}
